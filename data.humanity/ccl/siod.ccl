;; Helper functions and other stuff to get SIOD a bit more R5RS
;; compatible

(define (eof-object? port)
  (eq? port '()))

(define (ccl:load filename)
  (load filename #f #t))

(define else #t)

(define (display str)
  (writes nil str))

(display "Using Scheme: SIOD\n")

(gc-status #f)
